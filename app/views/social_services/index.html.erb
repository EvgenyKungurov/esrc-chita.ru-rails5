<% provide(:title, 'Социальная защита населения') %>

  <div class="row">
    <div class="alert alert-success" role="alert"><h3 class="center_align">Социальная защита населения</h3></div>
  </div>
  <p id="notice"><%= notice %></p>
    <table class="table">
      <% @social_services.sort_by { |service| service.position.to_i }.each do |social_service| %>
        <tr>
          <td><%= link_to social_service.name.html_safe, social_service %></td>
        </tr>
      <% end %>
    </table>
    <br>
